server {
    if ($host = www.duywebdev.click) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = duywebdev.click) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


        listen 80 ;
        listen [::]:80 ;
    server_name www.duywebdev.click duywebdev.click;
    return 404; # managed by Certbot
}

sudo systemctl start nginx